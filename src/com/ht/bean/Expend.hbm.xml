<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.ht.bean.Expend" table="expend" catalog="oa">
        <id name="payid">
            <column name="payid" />
            <generator class="native" />
        </id>
        <property name="paypro">
            <column name="paypro">
            </column>
        </property>
        <property name="paycount">
            <column name="paycount">
            </column>
        </property>
        <property name="ptime">
            <column name="ptime">
            </column>
        </property>
        <property name="payname">
            <column name="payname">
            </column>
        </property>
    </class>
</hibernate-mapping>
