<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.ht.bean.Expend" table="expend" catalog="oa">
        <id name="payid" type="java.lang.Integer">
            <column name="payid" />
            <generator class="identity" />
        </id>
        <property name="paypro" type="java.lang.String">
            <column name="paypro" length="1000">
            </column>
        </property>
        <property name="paycount" type="java.lang.Double">
            <column name="paycount">
            </column>
        </property>
        <property name="ptime" type="java.util.Date">
            <column name="ptime">
            </column>
        </property>
        <property name="payname" type="java.lang.String">
            <column name="payname" length="1000">
            </column>
        </property>
    </class>
</hibernate-mapping>
