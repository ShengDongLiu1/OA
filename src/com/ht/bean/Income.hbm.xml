<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.ht.bean.Income" table="income" catalog="oa">
        <id name="monid">
            <column name="monid" />
            <generator class="native" />
        </id>
        <property name="moncount">
            <column name="moncount" >
            </column>
        </property>
        <property name="monpro" >
            <column name="monpro">
            </column>
        </property>
        <property name="mname">
            <column name="mname">
            </column>
        </property>
        <property name="mdate">
            <column name="mdate">
            </column>
        </property>
    </class>
</hibernate-mapping>
